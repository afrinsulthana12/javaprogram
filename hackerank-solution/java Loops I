import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        // Read input N
        int N = sc.nextInt();
        
        // Loop to print multiplication table from 1 to 10
        for (int i = 1; i <= 10; i++) {
            System.out.println(N + " x " + i + " = " + (N * i));
        }
        
        sc.close();
    }
}
